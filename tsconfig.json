{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "ES6",
    "target": "ES5",
    "jsx": "react",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}